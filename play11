#include <iostream>
using namespace std;

int main()
{
  int a[30],i,sum=0,n;
  cout<<"enter the array size :";
  cin>>n;
  cout<<"enter the array elements ";
  for(i=0;i<n;i++)
  {
      cin>>a[i];
  }
  for(i=0;i<n;i++)
  {
      if(a[i]==i)
      {
          sum=sum+a[i]+i;
      }
  }
  cout<<sum;
}
